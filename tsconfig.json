{
  "compilerOptions": {
    "outDir": "public/js",
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react"
  },
  "include": ["./src/**/*"],
  "lib": ["es2015", "es2015.iterable", "dom"]
}
